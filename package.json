{
  "name": "react-universal",
  "version": "1.0.0",
  "description": "React Universal Template",
  "main": "index.js",
  "scripts": {
    "dev": "node --require babel-register index.js",
    "compile-server": "NODE_ENV=development webpack --config webpack/webpack.config.server.babel.js",
    "compile-client": "NODE_ENV=development webpack --config webpack/webpack.config.client.babel.js",
    "build": "npm run compile-server && npm run compile-client",
    "start": "node ./dist/server"
  },
  "keywords": [
    "react",
    "universal",
    "template",
    "redux"
  ],
  "author": "Alberto Forni",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "deep-equal": "^1.0.1",
    "express": "^4.13.4",
    "history": "^2.1.0",
    "invariant": "^2.2.1",
    "object-assign": "^4.1.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.4",
    "react-router-redux": "^4.0.2",
    "redux": "^3.4.0",
    "redux-thunk": "^2.0.1",
    "underscore": "^1.8.3",
    "warning": "^2.1.0"
  }
}
